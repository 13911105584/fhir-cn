          <max value="1"/>
          <type>
            <code value="string"/>
          </type>
          <isModifier value="false"/>
          <mapping>
            <identity value="v2"/>
            <map value="N/A"/>
          </mapping>
          <mapping>
            <identity value="rim"/>
            <map value="N/A - this is sometimes implied by context"/>
          </mapping>
        </definition>
      </element>
