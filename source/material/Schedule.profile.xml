          <short value="When the event occurs"/>
          <formal value="Identifies specific time periods when the event should occur."/>
          <min value="0"/>
          <max value="*"/>
          <type>
            <code value="Period"/>
          </type>
          <isModifier value="false"/>
          <mapping>
            <identity value="rim"/>
            <map value="QLIST&lt;TS&gt; or PIVL.offset"/>
          </mapping>
        </definition>
      </element>
      <element>
